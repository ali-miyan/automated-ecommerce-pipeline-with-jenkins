    <!-- New Product Section Start -->
    <div class="section section-padding">
        <div class="container">
            <!-- Product Section Wrapper Start -->
            <div class="product-section-wrapper">
                <div class="row justify-content-center">
                    <div class="col-lg-12">
                        <!-- Product Wrapper Start -->
                        <div class="product-wrapper product-active">
                            <!-- Product Top Wrapper Start -->
                            <div class="product-top-wrapper">
                                <!-- Section Title Start -->
                                <div class="section-title order-md-1">
                                    <h2 class="title"># New Products</h2>
                                </div>
                                <!-- Section Title End -->

                                <!-- Swiper Arrows End -->
                                <div class="swiper-arrows order-md-3">
                                    <!-- Add Arrows -->
                                    <div class="swiper-button-prev">
                                        <i class="pe-7s-angle-left"></i>
                                    </div>
                                    <div class="swiper-button-next">
                                        <i class="pe-7s-angle-right"></i>
                                    </div>
                                </div>
                                <!-- Swiper Arrows End -->
                            </div>
                            <!-- Product Top Wrapper End -->

                            <!-- Product Tabs Content Start -->
                            <div class="product-tabs-content">
                                <div class="tab-content">
                                    <div class="tab-pane fade show active" id="tab1">
                                        <div class="swiper-container">
                                            <div class="swiper-wrapper">
                                                <%console.log(product,'productssssssss')%>
                                                <% product.forEach(product=> { %>
                                                    <% if (!product.is_blocked && !product.isCategoryBlocked) { %>
                                                <div class="swiper-slide">
                                                    <!-- Single Product Start -->
                                                    <div class="single-product">
                                                        <a href="/detailshop?id=<%=product._id%>"><img src="multerimages/<%=product.images.image1 %>" width="270" height="303" alt="product" />
                                                            <% if (product.offer) { %>
                                                            <div style="position: absolute; top: 4px; left: 4px; background-color: #ffb300; color: #ffffff; padding: 5px; font-size: 12px;">
                                                                <%= Math.floor(((product.price-product.discountedPrice)/product.price)*100)%>% Off
                                                            </div>
                                                        <% } %></a>
                                                        <div class="product-content">
                                                            <h4 class="title">
                                                                <a href="/detailshop?id=<%=product._id%>">
                                                                    <%= product.name %>
                                                                </a>     
                                                            </h4>
                                                            <div class="price">
                                                                <%if(!product.discountedPrice){%>
                                                                    <span class="sale-price">₹<%=product.price%></span>
                                                                <%}else{%>
                                                                    <span class="old-price">₹<%=product.price%></span>&nbsp;&nbsp;<span class="sale-price">₹<%=product.discountedPrice%></span>
                                                                <%}%>
                                                            </div>
                                                        </div>
                                                    </div>
                                                  </div>
                                                <% } %>
                                            <% }); %>
                                                    <!-- Single Product End -->
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- Product Tabs Content End -->
                        </div>
                        <!-- Product Wrapper End -->
                    </div>
                </div>
            </div>
            <!-- Product Section Wrapper End -->
        </div>
    </div>
    <!-- New Product Section End -->

